<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<Platforms>x64;x86</Platforms>
		<PackageId>YDKK.Windows.NotifyIcon</PackageId>
		<Authors>YDKK</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Copyright>Copyright (c) 2022 YDKK</Copyright>
		<PackageProjectUrl></PackageProjectUrl>
		<RepositoryUrl>https://github.com/YDKK/YDKK.Windows.NotifyIcon</RepositoryUrl>
		<PackageTags>NotifyIcon</PackageTags>
		<RepositoryType>git</RepositoryType>
		<Description>Windows Shell NotifyIcon Library</Description>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Windows.CsWin32" Version="0.1.647-beta">
			<PrivateAssets>all</PrivateAssets>
		</PackageReference>
	</ItemGroup>

</Project>
